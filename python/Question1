class student:
	def __init__(self,name,age,branch):
		self.name=name
		self.age=age
		self.branch=branch
	def print_student(self):
		print("name",self.name)
		print("age",self.age)
		print("branch",self.branch)
student1=student("bob",12,"Engineering")
student1.print_student()

OUTPUT
('name', 'bob')
('age', 12)
('branch', 'Engineering')
